<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <system.serviceModel>
    <client>
      <!-- named endpoints because both use the same contract -->
      <endpoint address="net.tcp://localhost:8009/GeoService"
                binding="netTcpBinding"
                contract="GeoLib.Contracts.IGeoService"
                name="tcpEP"
                bindingConfiguration="tcpbinding1" />
      <endpoint address="http://localhost/GeoService"
                binding="basicHttpBinding"
                contract="GeoLib.Contracts.IGeoService"
                name="httpEP" />
      <endpoint address="net.tcp://localhost:8010/MessageService"
                binding="netTcpBinding"
                contract="GeoLib.Client.Contracts.IMessageService" />
    </client>
    <bindings>
      <netTcpBinding>
        <binding name="tcpbinding1" maxReceivedMessageSize="2000000" sendTimeout="00:00:05" receiveTimeout="00:20:00">
          <reliableSession inactivityTimeout="00:10:00" />
        </binding>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>
</configuration>